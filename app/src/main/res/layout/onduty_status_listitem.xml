<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="MissingConstraints,UselessLeaf">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/C8dp"
            android:elevation="@dimen/C5dp"
            app:cardCornerRadius="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/C12dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/sf_namelayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/C5dp"
                    android:visibility="gone">


                    <TextView
                        android:id="@+id/SfName"
                        style="@style/TextStyleOne"
                        android:text="-"
                        android:textColor="@android:color/black"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/ondutydate"
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="-"
                        android:padding="@dimen/C7dp"
                        android:textColor="@android:color/black"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/OStatus"
                        android:text="-"
                        style="@style/TextStyleOne"
                        android:padding="@dimen/C8dp"
                        android:textColor="@android:color/white" />

                </LinearLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_margin="@dimen/C5dp"
                    android:background="@android:color/black" />

      <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/C7dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/C7dp">

                    <TextView
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="Type"
                        android:textAllCaps="true"
                        android:textColor="@color/dark_greys"
                        android:textSize="11dp" />

                    <TextView
                        style="@style/TextStyleOne"
                        android:text="SHIFT"
                        android:textAllCaps="true"
                        android:textColor="@color/dark_greys"
                        android:textSize="11dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/type"
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="-"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/shift"
                        style="@style/TextStyleOne"
                        android:text="-"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                </LinearLayout>

                <TextView
                    style="@style/TextStyleOne"
                    android:layout_marginTop="@dimen/C8dp"
                    android:layout_weight="1"
                    android:text="LOCATION"
                    android:textAllCaps="true"
                    android:textColor="@color/dark_greys"
                    android:textSize="11dp" />

                <TextView
                    android:id="@+id/odlocation"
                    style="@style/TextStyleOne"
                    android:layout_weight="1"
                    android:text="-"
                    android:textAllCaps="true"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold" />


                <TextView
                    style="@style/TextStyleOne"
                    android:layout_marginTop="@dimen/C8dp"
                    android:layout_weight="1"
                    android:text="PURPOSE OF VISIT"
                    android:textAllCaps="true"
                    android:textColor="@color/dark_greys"
                    android:textSize="11dp" />


                <TextView
                    android:id="@+id/POV"
                    style="@style/TextStyleOne"
                    android:layout_weight="1"
                    android:text="-"
                    android:textAllCaps="true"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/C10dp">

                    <TextView
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="IN TIME"
                        android:textAllCaps="true"
                        android:textColor="@color/dark_greys"
                        android:textSize="11dp" />

                    <TextView
                        style="@style/TextStyleOne"
                        android:text="OUT TIME"
                        android:textAllCaps="true"
                        android:textColor="@color/dark_greys"
                        android:textSize="11dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/intime"
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="-"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/outtime"
                        style="@style/TextStyleOne"
                        android:text="-"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/C10dp">

                    <TextView
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="GEO IN"
                        android:textAllCaps="true"
                        android:textColor="@color/dark_greys"
                        android:textSize="11dp" />

                    <TextView
                        style="@style/TextStyleOne"
                        android:text="GEO OUT"
                        android:textAllCaps="true"
                        android:textColor="@color/dark_greys"
                        android:textSize="11dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/geoin"
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="-"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/geoout"
                        style="@style/TextStyleOne"
                        android:text="-"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/C7dp">

                    <TextView
                        android:id="@+id/applieddate"
                        style="@style/TextStyleOne"
                        android:layout_weight="1"
                        android:text="-"
                        android:textColor="@color/dark_greys"
                        android:textSize="13dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/Papproved"
                        style="@style/TextStyleOne"
                        android:text="-"
                        android:textAlignment="textEnd"
                        android:textColor="@color/dark_greys"
                        android:textSize="13dp"
                        android:textStyle="bold" />

                </LinearLayout>
      </LinearLayout>

            </LinearLayout>


        </androidx.cardview.widget.CardView>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>